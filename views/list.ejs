<%- include("header")  %> 
<body>

  <div class="box" id="heading">
    <h1><%= kindOfList %></h1>
    <h1><%= kindOfDay %></h1>
    

  </div>

  <div class="box">
    <% items.forEach(function(item, index){ %>
    <div class="item">
      <input type="checkbox" />
      <p><%= item %></p>
    </div>
    <% }); %>

    <form class="item" action= "<%= postAction %> "  method="post">
      <input name="newItem" type="text" placeholder="Add New To Do" autocomplete="off"/>
      <button type="submit" name="kindOfList" value="<%= kindOfList %>">+</button>
    </form>

  </div>
  <%- include("footer")  %> 